{
    "_properties": {
        "ELASTIC_SEARCH_HOST": "localhost",
        "ELASTIC_STORAGE_HOST": "localhost",
        "ELASTIC_CLUSTER": null,
        "ELASTIC_PORT": 9300,
        "AVAILABLE_TYPES": ["eld","def","sys"]
    },
    "_id": "mimer",
    "_docBaseUri" : "http://libris.kb.se/",
    "_supportplugins": [
        "elasticnode"
    ],
    "_plugins": {
        "_camel": [
            "indexroutebuilder"
        ],
        "_components": [
            "index",
            "mimerstorage",
            "readonly_librisstorage"
        ]
    },
    "_apis": [
        { "/_sys/metasearch":  "metasearchapi" },
        { "^/_iri/(.+)$": "documentapi" },
        { "^([^_].*)$":  "documentapi" },
        { "^/$":  "documentapi" }
    ]
}
