{
    "elasticprocessor": {
        "_class": "whelk.camel.ElasticRouteProcessor",
        "_plugins": [ "index" ]
    },
    "xinfoprocessor": {
      "_class": "whelk.camel.XInfoRouteProcessor",
      "_plugins": [ "index" ],
      "_params" : {
        "cherryElasticUrl": "localhost:9200/cherry"
      }

    },
    "postgrestorage": {
        "_class" : "whelk.component.PostgreSQLStorage",
        "_params" : {
            "availableTypes": "_property:AVAILABLE_TYPES",
            "databaseUrl": "_property:DATABASE_URL",
            "contentTypes": ["application/ld+json", "image/jpeg"],
            "versioning" : false
        }
    }
}
