{
    "_properties": {
        "ELASTIC_STORAGE_HOST": "hp01.libris.kb.se",
        "GRAPHSTORE_DATA_URI": "http://hp04.libris.kb.se:8890/sparql-graph-crud",
        "GRAPHSTORE_QUERY_URI": "http://hp04.libris.kb.se:8890/sparql",
        "GRAPHSTORE_UPDATE_URI": "http://hp04.libris.kb.se:8890/sparql-auth",
        "GRAPHSTORE_UPDATE_POST_PARAMETER": "update",
        "GRAPHSTORE_UPDATE_AUTHENTICATION_REQUIRED": true,
        "APIX_HOST": "https://libris.kb.se",
        "UPDATE_OAIPMH_URL":"http://data.libris.kb.se/{dataset}/oaipmh",
        "UPDATE_HOLD_OAIPMH_URL":"http://data.libris.kb.se/{dataset}/oaipmh",
        "UPDATE_INTERVAL":1
    },
    "_plugins": {
        "_camel": [
            "indexroutebuilder",
            "apixroutebuilder"
        ],
        "_components": [
            "jsonldstorage",
            "index"
        ]
    },
    "_apis": [
        { "/_remotesearch": "remotesearch" },
        { "/_format": "formatter" },
        { "/_libcount": "libcount" },
        { "/_isxntool":  "isxntool" },
        { "/_complete/([\\w/]+)$":  "completeexpander" },
        { "/(\\w+)/_search":  "search" },
        { "^/_iri/(.+)$": "documentapi" },
        { "^([^_].*)$":  "documentapi" },
        { "^/$":  "documentapi" }
    ]
}
