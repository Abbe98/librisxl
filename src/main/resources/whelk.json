{
    "_properties": {
        "ELASTIC_SEARCH_HOST": "hp01.libris.kb.se:9300",
        "ELASTIC_CLUSTER": "whelks",
        "ELASTIC_INDEX_NAME": "libris",
        "AVAILABLE_TYPES": ["auth","bib","hold","def","sys"],
        "ACTIVEMQ_BROKER_URL": "tcp://hp04.libris.kb.se:61616?wireFormat.maxInactivityDuration=3600000",
        "MQ_INDEX": "activemq:{whelkname}.index?concurrentConsumers=10",
        "MQ_BULK_INDEX": "activemq:{whelkname}.bulk.index?concurrentConsumers=10",
        "MQ_GRAPHSTORE": "activemq:{whelkname}.graphstore",
        "MQ_GRAPHSTORE_RETRIES": "activemq:{whelkname}.graphstore.retries",
        "MQ_APIX": "activemq:{whelkname}.apix",
        "MQ_APIX_RETRIES": "activemq:{whelkname}.apix.retries",
        "APIX_PATH": "/apix/0.1/cat/test/",
        "CAMEL_MASTER_COMPONENT":"sendmq",
        "CAMEL_COMPONENT_CONFIG":"concurrentConsumers=5",
        "UPDATE_OAIPMH_URL":"http://data.libris.kb.se/{dataset}/oaipmh",
        "UPDATE_HOLD_OAIPMH_URL":"http://data.libris.kb.se/{dataset}/oaipmh",
        "UPDATE_INTERVAL":300,
        "DATABASE_URL": "jdbc:postgresql://hp04.libris.kb.se:5432/whelk"
    },
    "_id": "libris",
    "_docBaseUri" : "http://libris.kb.se/",
    "_plugins": {
        "_camel": [
            "indexroutebuilder"
            ],
        "_components": [
            "postgrestorage",
            "index"
        ],
        "_maintenance": [
            "scheduledoperator"
        ],
        "_filters": [
            "linkexpander"
        ]
    },
    "_apis": [
        { "/_remotesearch": "remotesearch" },
        { "/_operations": "operator" },
        { "/_format": "formatter" },
        { "/_sys/metasearch":  "metasearchapi" },
        { "/_libcount": "libcount" },
        { "/_isxntool":  "isxntool" },
        { "/_complete/([\\w/]+)$":  "completeexpander" },
        { "/([\\w/]+)/_oaipmhsearch": "oaipmhsearch"},
        { "/([\\w/]+)/_search":  "search" },
        { "^/_iri/(.+)$": "documentapi" },
        { "^([^_].*)$":  "documentapi" },
        { "^/$":  "documentapi" }
    ]
}
