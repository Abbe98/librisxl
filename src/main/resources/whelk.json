{
    "_properties": {
        "ELASTIC_SEARCH_HOST": "hp01.libris.kb.se",
        "ELASTIC_STORAGE_HOST": "hp01.libris.kb.se",
        "ELASTIC_CLUSTER": "whelks",
        "ELASTIC_PORT": 9300,
        "AVAILABLE_TYPES": ["auth","bib","hold","def","sys"],
        "ACTIVEMQ_BROKER_URL": "tcp://hp04.libris.kb.se:61616?wireFormat.maxInactivityDuration=3600000",
        "MQ_INDEX": "activemq:{whelkname}.index?concurrentConsumers=10",
        "MQ_BULK_INDEX": "activemq:{whelkname}.bulk.index?concurrentConsumers=10",
        "MQ_GRAPHSTORE": "activemq:{whelkname}.graphstore",
        "MQ_GRAPHSTORE_RETRIES": "activemq:{whelkname}.graphstore.retries",
        "MQ_APIX": "activemq:{whelkname}.apix",
        "MQ_APIX_RETRIES": "activemq:{whelkname}.apix.retries",
        "CAMEL_MASTER_COMPONENT":"sendmq",
        "CAMEL_COMPONENT_CONFIG":"concurrentConsumers=10",
        "GRAPHSTORE_DATA_URI": "http://localhost:3030/libris/data",
        "GRAPHSTORE_QUERY_URI": "http://localhost:3030/libris/query",
        "GRAPHSTORE_UPDATE_URI": "http://localhost:3030/libris/update",
        "GRAPHSTORE_UPDATE_AUTHENTICATION_REQUIRED": false,
        "UPDATE_OAIPMH_URL":"http://localhost:8000/{dataset}/oaipmh",
        "UPDATE_INTERVAL":300,
        "ES_CONFIG_PREFIX": "libris"
    },
    "_id": "libris",
    "_docBaseUri" : "http://libris.kb.se/",
    "_docMetaMapping" : {
        "hold": [
            "about.heldBy.notation"
        ]
    },
    "_plugins": {
        "_camel": [
            "indexroutebuilder",
            "graphstoreroutebuilder"
            ],
        "_components": [
            "jsonldstorage",
            "index",
            "graphstore"
        ],
        "_maintenance": [
            "scheduledoperator"
        ],
        "_filters": [
            "linkexpander"
        ]
    },
    "_apis": [
        { "/_remotesearch": "remotesearch" },
        { "/_operations": "operator" },
        { "/_format": "formatter" },
        { "/_sparql": "sparqlapi" },
        { "/_sys/metasearch":  "metasearchapi" },
        { "/_libcount": "libcount" },
        { "/_isxntool":  "isxntool" },
        { "/_complete/([\\w/]+)$":  "completeexpander" },
        { "/([\\w/]+)/_oaipmhsearch": "oaipmhsearch"},
        { "/([\\w/]+)/_search":  "search" },
        { "^/_iri/(.+)$": "documentapi" },
        { "^([^_].*)$":  "documentapi" },
        { "^/$":  "documentapi" }
    ]
}
