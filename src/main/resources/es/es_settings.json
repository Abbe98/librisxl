{
    "analysis" : {
        "analyzer" : {
            "default" : {
                "tokenizer" : "standard",
                "filter" : ["lowercase", "swe_light_stemmer", "additional_asciifolding"],
                "stopwords" : "_none_"
            },
            "stemmed" : {
              "tokenizer" : "standard",
              "filter" :  ["lowercase", "swe_light_stemmer"]
            },
            "completer" : {
                "type" : "custom",
                "tokenizer" : "lowercase",
                "filter" : "custom_edge_ngram"
            }

        },
        "filter" : {
            "swe_light_stemmer" : {
                "type" : "stemmer",
                "name" : "light_swedish"
            },
            "custom_edge_ngram" : {
                "type" : "edgeNGram",
                "min_gram" : 2,
                "max_gram" : 50
            },
            "additional_asciifolding" : {
              "type" : "asciifolding",
              "preserve_original" : true
            }
        }
    }
}
