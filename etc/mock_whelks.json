{
    "_whelks": [
        {
            "libris":{
                "_class" : "se.kb.libris.whelks.http.HttpStandardWhelk",
                "_plugins": [
                    "localindex",
                    "diskstorage",
                    "search",
                    "kitinsearch",
                    "metasearch",
                    "documentapi",
                    "bibmarcconverter",
                    "authmarcconverter",
                    "holdmarcconverter",
                    "cleanupindexconverter",
                    "cleanupconverter",
                    "entityextractor",
                    "jsonldlinkfinder",
                    "autocomplete" ],
                "contentRoot" : "/"
            }
        },
        {
            "resource":{
                "_class" : "se.kb.libris.whelks.ResourceWhelk",
                "_plugins" : ["diskstorage", "documentapi", "resourcelist", "marcmap", "search", "presentationformatter"]
            }
        }
    ],
    "_plugins": [
        {
            "metasearch": {
                "_class": "se.kb.libris.whelks.api.MetadataSearchRestlet"
            }
        },
        {
            "resourcelist": {
                "_class": "se.kb.libris.whelks.api.ResourceListRestlet"
            }
        },
        {
            "marcmap": {
                "_class": "se.kb.libris.whelks.api.MarcMapRestlet"
            }
        },
        {
            "presentationformatter": {
                "_class": "se.kb.libris.whelks.api.PresentationFormatter"
            }
        },
        {
            "localindex": {
                "_class" : "se.kb.libris.whelks.component.ElasticSearchNode",
                "_params" : "work/data",
                "_unique" : false
            }
        },
        {
            "index": {
                "_class" : "se.kb.libris.whelks.component.ElasticSearchClientIndex",
                "_unique" : false
            }
        },
        {
            "cassandrastorage": {
                "_class" : "se.kb.libris.whelks.component.CassandraStorage", 
                "_priority" : 0
            }
        },
        {
            "diskstorage": {
                "_class" : "se.kb.libris.whelks.component.DiskStorage", 
                "_params" : "work/storage",
                "_priority" : 1
            }
        },
        {
            "marcmassager" : {
                "_class" : "se.kb.libris.whelks.plugin.MarcCrackerAndLabelerIndexFormatConverter"
            }
        },
        {
            "bibmarcconverter" : {
                "_class" : "se.kb.libris.whelks.plugin.MarcBib2JsonLDConverter",
                "_params" : "bib"
            }
        },
        {
            "holdmarcconverter" : {
                "_class" : "se.kb.libris.whelks.plugin.Marc2JsonLDConverter",
                "_params" : "hold"
            }
        },
        {
            "authmarcconverter" : {
                "_class" : "se.kb.libris.whelks.plugin.MarcAuth2JsonLDConverter",
                "_params" : "auth"
            }
        },
        {
            "cleanupconverter" : {
                "_class" : "se.kb.libris.whelks.plugin.JsonLDCleanupFormatConverter",
                "_priority" : 0
            }
        },
        {
            "cleanupindexconverter" : {
                "_class" : "se.kb.libris.whelks.plugin.JsonLDCleanupIndexFormatConverter",
                "_priority" : 1
            }
        },
        {
            "entityextractor" : {
                "_class" : "se.kb.libris.whelks.plugin.JsonLDEntityExtractorIndexFormatConverter",
                "_priority" : 2
            }
        },
        {
            "search" : {
                "_class" : "se.kb.libris.whelks.api.SearchRestlet",
                "_params" : {"boost" : "labels.title:2"}
            }
        },
        {
            "documentapi" : {
                "_class" : "se.kb.libris.whelks.api.DocumentRestlet"
            }
        },
        {
            "kitinsearch" : {
                "_class" : "se.kb.libris.whelks.api.KitinSearchRestlet2"
            }
        },
        {
            "autocomplete" : {
                "_class" : "se.kb.libris.whelks.api.AutoComplete",
                "_params" : {"queryFields":[
                              "authoritativeName",
                              "label"
                            ],
                            "infoFields":[
                              "birthYear",
                              "deathYear",
                              "authorOf",
                              "hasAnnotation.about"
                            ],
                            "indexTypes":"person"
                            }
            }
        },
        {
            "subjectautocomplete" : {
                "_class" : "se.kb.libris.whelks.api.AutoComplete",
                "_params" : {"queryFields":[
                              "term"
                            ],
                            "infoFields":[],
                            "indexTypes":"subject"
                            }
            }
        },
        {
            "suggestformatconverter" : {
                "_class" : "se.kb.libris.whelks.plugin.external.AutoSuggestFormatConverter",
                "_params" : "_whelk:bib"
            }
        }, 
        {
            "biblistener" : {
                "_class" : "se.kb.libris.whelks.plugin.Listener",
                "_params" : "_whelk:bib"
            }
        },
        {
            "authlistener" : {
                "_class" : "se.kb.libris.whelks.plugin.Listener",
                "_params" : "_whelk:auth"
            }
        },
        {
            "jsonldlinkfinder" : {
                "_class" : "se.kb.libris.whelks.plugin.JsonLDLinkFinder"
            }
        }
    ]
}
