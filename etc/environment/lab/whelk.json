{
    "_properties": {
        "ELASTIC_SEARCH_HOST": "hp01.libris.kb.se",
        "ELASTIC_STORAGE_HOST": "hp01.libris.kb.se",
        "ELASTIC_CLUSTER": "whelks",
        "ELASTIC_PORT": 9300,
        "AVAILABLE_TYPES": ["auth","bib","hold","def","sys"],
        "ACTIVEMQ_BROKER_URL": "tcp://hp04.libris.kb.se:61616?wireFormat.maxInactivityDuration=3600000",
        "APIX_HOST": "https://libris.kb.se",
        "MQ_INDEX": "activemq:testlibris.index",
        "MQ_BULK_INDEX": "activemq:testlibris.bulk.index?concurrentConsumers=10",
        "MQ_APIX": "activemq:testlibris.apix",
        "MQ_APIX_RETRIES": "activemq:testlibris.apix.retries",
        "CAMEL_MASTER_COMPONENT":"sendmq",
        "CAMEL_CHANNEL_PREFIX":"test",
        "CAMEL_COMPONENT_CONFIG":"concurrentConsumers=10",
        "UPDATE_OAIPMH_URL":"http://averell.libris.kb.se/vcopy/{dataset}/oaipmh",
        "UPDATE_INTERVAL": 30
    },
    "_id": "testlibris",
    "_docBaseUri" : "http://libris.kb.se/",
    "_plugins": {
        "_camel": [
            "indexroutebuilder",
            "apixroutebuilder"
            ],
        "_components": [
            "jsonldstorage",
            "index"
        ],
        "_maintenance": [
            "scheduledoperator"
        ]
    },
    "_apis": [
        { "/_remotesearch": "remotesearch" },
        { "/_operations": "operator" },
        { "/_format": "formatter" },
        { "/_sparql": "sparqlapi" },
        { "/_sys/metasearch":  "metasearchapi" },
        { "/_libcount": "libcount" },
        { "/_isxntool":  "isxntool" },
        { "/_complete/([\\w/]+)$":  "completeexpander" },
        { "/(\\w+)/_search":  "search" },
        { "^([^_].*)$":  "documentapi" },
        { "^/$":  "documentapi" }
    ]
}

