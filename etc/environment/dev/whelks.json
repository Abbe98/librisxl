{
    "_whelks": [
        {
            "libris":{
                "_class" : "se.kb.libris.whelks.http.HttpStandardWhelk",
                "_plugins": [
                    "localindex",
                    "flatstorage",
                    "search",
                    "sparqlapi",
                    "remotesearch",
                    "documentapi",
                    "marcconverter",
                    "turtleconverter",
                    "entityextractor",
                    "linkenhancer",
                    "jsonldlinkfinder",
                    "nameexpander"
                ],
                "contentRoot" : "/",
                "docBaseUri" : "http://libris.kb.se/"
            }
        },
        {
            "resource":{
                "_class" : "se.kb.libris.whelks.http.HttpStandardWhelk",
                "_plugins" : ["diskstorage", "documentapi", "resourcelist", "marcmap", "search", "isxntool"]
            }
        }
    ],
    "_plugins": {
        "localindex": {
            "_class" : "se.kb.libris.whelks.component.ElasticSearchNode",
            "_params" : "work/data",
            "_unique" : false
        }
    }
}
