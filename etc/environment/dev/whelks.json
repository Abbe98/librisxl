{
    "_whelks": [
    {
        "def":{
            "_class" : "se.kb.libris.whelks.http.HttpStandardWhelk",
            "_plugins" : [
                "flatstorage",
                "localindex",
                "fusekigraphstore",
                "turtleconverter",
                "defsindexconverter",
                "search",
                "documentapi"
                ],
            "docBaseUri" : "http://libris.kb.se/"
        }
    },
    {
        "libris":{
            "_class" : "se.kb.libris.whelks.http.HttpStandardWhelk",
            "_plugins": [
                "localindex",
                "flatstorage",
                "fusekigraphstore",
                "search",
                "oaipmhxmlconverter",
                "completeexpander",
                "sparqlapi",
                "remotesearch",
                "libcount",
                "documentapi",
                "marcconverter",
                "turtleconverter",
                "entityextractor",
                "linkcompleter",
                "indexlinkfinder",
                "oaipmhimporter",
                "marcimporter"
                ],
            "contentRoot" : "/",
            "docBaseUri" : "http://libris.kb.se/"
        }
    },
    {
        "swepub":{
            "_class" : "se.kb.libris.whelks.http.HttpStandardWhelk",
            "_plugins" : [
                "swepubstorage",
                "documentapi",
                "modsimporter"
                ]
        }
    },
    {
        "resource":{
            "_class" : "se.kb.libris.whelks.http.HttpStandardWhelk",
            "_plugins" : [
                "marcmap",
            "isxntool"
                ]
        }
    }
    ],
        "_plugins": {
            "localindex": {
                "_class" : "se.kb.libris.whelks.component.ElasticSearchNode",
                "_params" : "work/data",
                "_unique" : true
            },
            "flatstorage": {
                "_class" : "se.kb.libris.whelks.component.FlatDiskStorage",
                "_params" : {
                    "storageDir": "work/storage/flat/main",
                    "contentType": "application/ld+json"
                }
            },
            "swepubstorage": {
                "_class" : "se.kb.libris.whelks.component.FlatDiskStorage",
                "_params" : {
                    "storageDir": "work/storage/flat/swepub",
                    "contentType": "application/mods+xml"
                }
            },
            "diskstorage": {
                "_class" : "se.kb.libris.whelks.component.DiskStorage",
                "_params" : {
                    "storageDir": "work/storage/disk/jsonld",
                    "contentType": "application/ld+json"
                }
            },
            "localdefinitiondata": {
                "_class": "se.kb.libris.whelks.api.DefinitionDataRestlet",
                "_params": "datatools/build/"
            }
        }
}
