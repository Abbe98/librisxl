{
    "_properties": {
        "WHELK_WORK_DIR": "work",
        "STORAGE_PATH": "work/storage",
        "ELASTIC_HOST": "localhost",
        "ELASTIC_STORAGE_HOST": "localhost",
        "ELASTIC_PORT": 9300,
        "GRAPHSTORE_DATA_URI": "http://localhost:3030/libris/data",
        "GRAPHSTORE_QUERY_URI": "http://localhost:3030/libris/query",
        "GRAPHSTORE_UPDATE_URI": "http://localhost:8890/sparql-graph-crud",
        "GRAPHSTORE_UPDATE_URI": "http://localhost:3030/libris/update",
        "ACTIVEMQ_BROKER_URL": "tcp://localhost:61616",
        "MQ_INDEX": "activemq:libris.index",
        "MQ_BULK_INDEX": "activemq:libris.bulk.index?concurrentConsumers=5",
        "MQ_GRAPHSTORE": "activemq:libris.graphstore"
    },
    "_id": "libris",
    "_docBaseUri" : "http://libris.kb.se/",
    "_plugins": {
        "_support": [
            "elasticnode",
            "activemqbroker",
            "fusekiserver"
            ],
        "_camel": [
            "routebuilder"
            ],
        "_components": [
            "esjsonldstorage",
            "jsonldstorage",
            "index",
            "graphstore"
        ]
    },
    "_apis": [
        { "/_remotesearch": "remotesearch" },
        { "/_operations": "operator" },
        { "/_format": "formatter" },
        { "/_sparql": "sparqlapi" },
        { "/_libcount": "libcount" },
        { "/_isxntool":  "isxntool" },
        { "/_complete/([\\w/]+)$":  "completeexpander" },
        { "/([\\w/]+)/_search":  "search" },
        { "^/([^_].*)$":  "documentapi" },
        { "^/$":  "documentapi" }
    ]
}
