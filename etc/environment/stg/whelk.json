{
    "_properties": {
        "WHELK_WORK_DIR": "/var/run/whelk",
        "STORAGE_PATH": "/data/librisxl/storage",
        "ELASTIC_HOST": "localhost",
        "ELASTIC_PORT": 9300,
        "GRAPHSTORE_DATA_URI": "http://localhost:3030/libris/data",
        "GRAPHSTORE_QUERY_URI": "http://localhost:3030/libris/query",
        "GRAPHSTORE_UPDATE_URI": "http://localhost:3030/libris/update"
    },
    "_id": "libris",
    "_docBaseUri" : "http://libris.kb.se/",
    "_plugins": {
        "_expanders": [
            "linkexpander"
            ],
        "_importers": [
            "oaipmhimporter",
        "marcimporter"
            ],
        "_components": [
            "pairtreehybridstorage",
            "pairtreeoaistorage",
            "index",
            "graphstore"
        ]
    },
    "_apis": [
        { "/_remotesearch": "remotesearch" },
        { "/_operations": "operator" },
        { "/_format": "formatter" },
        { "/_sparql": "sparqlapi" },
        { "/_libcount": "libcount" },
        { "/_isxntool":  "isxntool" },
        { "/_complete/([\\w/]+)$":  "completeexpander" },
        { "/(\\w+)/_search":  "search" },
        { "^/([^_].+)$":  "documentapi" },
        { "^/$":  "documentapi" }
    ]
}
