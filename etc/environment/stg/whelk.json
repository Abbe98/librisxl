{
    "_properties": {
        "WHELK_WORK_DIR": "/var/run/whelk",
        "STORAGE_PATH": "/data/librisxl/storage",
        "ELASTIC_SEARCH_HOST": "hp01.libris.kb.se",
        "ELASTIC_STORAGE_HOST": "hp01.libris.kb.se",
        "ELASTIC_CLUSTER": "whelks",
        "ELASTIC_PORT": 9300,
        "AVAILABLE_TYPES": ["auth","bib","hold","def","sys"],
        "ACTIVEMQ_BROKER_URL": "tcp://hp04.libris.kb.se:61616?wireFormat.maxInactivityDuration=0",
        "MQ_INDEX": "activemq:stglibris.index?concurrentConsumers=10",
        "MQ_BULK_INDEX": "activemq:stglibris.bulk.index?concurrentConsumers=10",
        "MQ_GRAPHSTORE": "activemq:stglibris.graphstore?concurrentConsumers=10",
        "MQ_GRAPHSTORE_RETRIES": "activemq:stglibris.graphstore.retries",
        "CAMEL_MASTER_COMPONENT":"sendmq",
        "CAMEL_CHANNEL_PREFIX":"stg",
        "CAMEL_COMPONENT_CONFIG":"concurrentConsumers=50",
        "UPDATE_OAIPMH_URL":"http://data.libris.kb.se/{dataset}/oaipmh",
        "UPDATE_INTERVAL":30
    },
    "_id": "stglibris",
    "_docBaseUri" : "http://libris.kb.se/",
    "_plugins": {
        "_camel": [
            "indexroutebuilder"
            ],
        "_components": [
            "jsonldstorage",
            "index"
        ],
        "_maintenance": [
            "scheduledoperator"
        ]
    },
    "_apis": [
        { "/_remotesearch": "remotesearch" },
        { "/_format": "formatter" },
        { "/_sparql": "sparqlapi" },
        { "/_sys/metasearch":  "metasearchapi" },
        { "/_libcount": "libcount" },
        { "/_isxntool":  "isxntool" },
        { "/_complete/([\\w/]+)$":  "completeexpander" },
        { "/(\\w+)/_search":  "search" },
        { "^([^_].*)$":  "documentapi" },
        { "^/$":  "documentapi" }
    ]
}
