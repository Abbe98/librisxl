{
    "_whelks": [
        {
            "defterms":{
                "_class" : "se.kb.libris.whelks.http.HttpStandardWhelk",
                "_plugins" : [
                    "definitiondata"
                    ],
                "contentRoot" : "/def/term",
                "docBaseUri" : "http://libris.kb.se/"
            }
        },
        {
            "def":{
                "_class" : "se.kb.libris.whelks.http.HttpStandardWhelk",
                "_plugins" : [
                    "pairtree",
                    "index",
                    "fuseki",
                    "turtleconverter",
                    "defsindexconverter",
                    "search",
                    "documentapi"
                    ],
                "docBaseUri" : "http://libris.kb.se/"
            }
        },
        {
            "libris":{
                "_class" : "se.kb.libris.whelks.http.HttpStandardWhelk",
                "_converters": [
                    "marcconverter",
                    "turtleconverter"
                ],
                "_expanders": [
                    "linkexpander"
                ],
                "_importers": [
                    "oaipmhimporter"
                ],
                "_apis": [
                    "remotesearch",
                    "operator",
                    "search",
                    "completeexpander",
                    "isxntool",
                    "sparqlapi",
                    "libcount",
                    "documentapi"
                ],
                "_otherplugins": [
                    "pairtreehybrid",
                    "index",
                    "fuseki",
                    "entityextractor",
                    "linkcompleter",
                    "indexlinkfinder"
                ],
                "contentRoot" : "/",
                "docBaseUri" : "http://libris.kb.se/"
            }
        }
    ],
    "_plugins": {
        "pairtree": {
            "_class" : "se.kb.libris.whelks.component.PairtreeDiskStorage",
            "_params" : {
                "storageDir": "/data/librisxl/storage",
                "contentTypes": ["application/ld+json"]
            }
        },
        "pairtreehybrid": {
            "_class" : "se.kb.libris.whelks.component.PairtreeHybridDiskStorage",
            "_plugins" : [
                "index"
                ],
            "_params" : {
                "storageDir": "/data/librisxl/storage",
                "contentTypes": ["application/ld+json"],
                "baseStorageSuffix": "pairtree",
                "versioning" : false
            }
        },
        "fuseki": {
            "_class" : "se.kb.libris.whelks.component.HttpBatchGraphStore",
            "_params" : {
                "graphStoreUri": "http://localhost:3030/libris/data",
                "queryUri": "http://localhost:3030/libris/query",
                "updateUri": "http://localhost:3030/libris/update",
                "contextPath": "context.jsonld",
                "baseUri": "http://libris.kb.se/resource/"
            }
        }
    }
}
