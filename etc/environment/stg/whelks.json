{
    "_whelks": [
        {
            "libris":{
                "_class" : "se.kb.libris.whelks.http.HttpStandardWhelk",
                "_converters": [
                    "oaipmhxmlconverter",
                    "marcconverter",
                    "turtleconverter"
                ],
                "_importers": [
                    "oaipmhimporter",
                    "marcimporter"
                ],
                "_apis": [
                    "remotesearch",
                    "operator",
                    "search",
                    "sparqlapi",
                    "libcount",
                    "completeexpander",
                    "documentapi"
                ],
                "_otherplugins": [
                    "index",
                    "pairtree",
                    "cassandrastorage",
                    "oaipmhstorage",
                    "entityextractor",
                    "linkcompleter",
                    "indexlinkfinder"
                ],
                "contentRoot" : "/",
                "docBaseUri" : "http://libris.kb.se/"
            }
        },
        {
            "swepub":{
                "_class" : "se.kb.libris.whelks.http.HttpStandardWhelk",
                "_plugins" : [
                    "cassandrastorage",
                    "documentapi",
                    "modsimporter"
                    ]
            }
        },
        {
            "resource":{
                "_class" : "se.kb.libris.whelks.http.HttpStandardWhelk",
                "_plugins" : [
                    "definitiondata",
                    "resourcelist",
                    "marcmap",
                    "isxntool"
                ]
            }
        }
    ],
    "_plugins": {
        "pairtree": {
            "_class" : "se.kb.libris.whelks.component.PairtreeDiskStorage",
            "_params" : {
                "storageDir": "/data/librisxl/storage",
                "contentTypes": ["application/ld+json"],
                "versioning" : true
            }
        }
    }
}
