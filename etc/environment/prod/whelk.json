        "GRAPHSTORE_UPDATE_URI": "http://hp03.libris.kb.se:3030/libris/update",
        "ACTIVEMQ_BROKER_URL": "tcp://hp02.libris.kb.se:61616?wireFormat.maxInactivityDuration=3600000",
