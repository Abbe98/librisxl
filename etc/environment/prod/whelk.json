{
    "_properties": {
        "WHELK_WORK_DIR": "/var/run/whelk",
        "ELASTIC_SEARCH_HOST": "hp01.libris.kb.se",
        "ELASTIC_STORAGE_HOST": "hp01.libris.kb.se",
        "ELASTIC_CLUSTER": "whelks",
        "ELASTIC_PORT": 9300,
        "AVAILABLE_TYPES": ["auth","bib","hold","def","sys"],
        "GRAPHSTORE_DATA_URI": "http://hp04.libris.kb.se:8890/sparql-graph-crud",
        "GRAPHSTORE_QUERY_URI": "http://hp04.libris.kb.se:8890/sparql",
        "GRAPHSTORE_UPDATE_URI": "http://hp04.libris.kb.se:8890/sparql-auth",
        "GRAPHSTORE_UPDATE_POST_PARAMETER": "update",
        "GRAPHSTORE_UPDATE_AUTHENTICATION_REQUIRED": true,
        "ACTIVEMQ_BROKER_URL": "tcp://hp04.libris.kb.se:61616?wireFormat.maxInactivityDuration=0",
        "MQ_INDEX": "activemq:libris.index?concurrentConsumers=10",
        "MQ_BULK_INDEX": "activemq:libris.bulk.index?concurrentConsumers=10",
        "MQ_GRAPHSTORE": "activemq:libris.graphstore?concurrentConsumers=10"
        "MQ_APIX": "activemq:apix",
        "CAMEL_MASTER_COMPONENT":"sendmq",
        "CAMEL_COMPONENT_CONFIG":"concurrentConsumers=50"
    },
    "_id": "libris",
    "_docBaseUri" : "http://libris.kb.se/",
    "_docMetaMapping" : {
        "hold": {
            "sigel" : "about.heldBy.notation"
        }
    },
    "_plugins": {
        "_camel": [
            "indexroutebuilder",
            "graphstoreroutebuilder",
            "apixroutebuilder"
            ],
        "_components": [
            "jsonldstorage",
            "index",
            "graphstore"
        ]
    },
    "_apis": [
        { "/_remotesearch": "remotesearch" },
        { "/_operations": "operator" },
        { "/_format": "formatter" },
        { "/_sparql": "sparqlapi" },
        { "/_libcount": "libcount" },
        { "/_isxntool":  "isxntool" },
        { "/_complete/([\\w/]+)$":  "completeexpander" },
        { "/(\\w+)/_search":  "search" },
        { "^([^_].*)$":  "documentapi" },
        { "^/$":  "documentapi" }
    ]
}
