{
    "_properties": {
        "WHELK_WORK_DIR": "/var/run/whelk",
        "STORAGE_PATH": "/ssd/storage"
    },
    "_id": "libris",
    "_docBaseUri" : "http://libris.kb.se/",
    "_plugins": {
        "_converters": [
            "linkcompleter",
            "indexlinkfinder"
            ],
        "_expanders": [
            "linkexpander"
            ],
        "_importers": [
            "oaipmhimporter",
        "marcimporter"
            ],
        "_components": [
            "pairtreehybridstorage",
            "pairtreeoaistorage",
            "index"
        ]
    },
    "_masterComponent": "pairtreehybridstorage",
    "_apis": [
        { "/_remotesearch": "remotesearch" },
        { "/_operations": "operator" },
        { "/_format": "formatter" },
        { "/_sparql": "sparqlapi" },
        { "/_libcount": "libcount" },
        { "/_isxntool":  "isxntool" },
        { "/_complete/([\\w/]+)$":  "completeexpander" },
        { "/(\\w+)/_search":  "search" },
        { "^/([^_].+)$":  "documentapi" },
        { "^/$":  "documentapi" }
    ]
}
