{
    "_whelks": [
        {
            "defterms":{
                "_class" : "se.kb.libris.whelks.http.HttpStandardWhelk",
                "_plugins" : [
                    "definitiondata"
                    ],
                "contentRoot" : "/def/term",
                "docBaseUri" : "http://libris.kb.se/"
            }
        },
        {
            "def":{
                "_class" : "se.kb.libris.whelks.http.HttpStandardWhelk",
                "_plugins" : [
                    "pairtreediskstorage",
                    "index",
                    "fuseki",
                    "turtleconverter",
                    "defsindexconverter",
                    "search",
                    "documentapi"
                    ],
                "docBaseUri" : "http://libris.kb.se/"
            }
        },
        {
            "libris":{
                "_class" : "se.kb.libris.whelks.http.HttpStandardWhelk",
                "_converters": [
                    "marcconverter",
                    "turtleconverter"
                ],
                "_expanders": [
                    "linkexpander"
                ],
                "_importers": [
                    "oaipmhimporter",
                    "marcimporter"
                ],
                "_apis": [
                    "operator",
                    "remotesearch",
                    "search",
                    "completeexpander",
                    "isxntool",
                    "sparqlapi",
                    "libcount",
                    "documentapi"
                ],
                "_otherplugins": [
                    "index",
                    "pairtreehybridstorage",
                    "fuseki",
                    "entityextractor",
                    "linkcompleter",
                    "indexlinkfinder"
                ],
                "contentRoot" : "/",
                "docBaseUri" : "http://libris.kb.se/"
            }
        }
    ],
    "_plugins": {
        "fuseki": {
            "_class" : "se.kb.libris.whelks.component.HttpGraphStore",
            "_params" : {
                "graphStoreUri": "http://hp02:3030/libris/data",
                "queryUri": "http://hp02:3030/libris/query"
            }
        }
    }
}
