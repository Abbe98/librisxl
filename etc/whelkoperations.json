{
    "_whelks": [
        {
            "bib":{
                "_plugins": ["localindex", "diskstorage", "bibmarcconverter", "cleanupconverter", "entityextractor", "jsonldlinkfinder"]
            }
        },
        {
            "auth":{
                "_plugins": [ "localindex", "diskstorage", "authmarcconverter", "entityextractor", "jsonldlinkfinder" ]
            }
        },
        {
            "hold":{
                "_plugins": [ "localindex", "diskstorage", "holdmarcconverter", "jsonldlinkfinder" ]
            }
        }
    ],
    "_plugins": [
        {
            "localindex": {
                "_class" : "se.kb.libris.whelks.component.ElasticSearchNode",
                "_params" : "work/data",
                "_unique" : false
            }
        },
        {
            "diskstorage": {
                "_class" : "se.kb.libris.whelks.component.DiskStorage", 
                "_params" : "work/storage",
                "_priority" : 0
            }
        },
        {
            "marcmassager" : {
                "_class" : "se.kb.libris.whelks.plugin.MarcCrackerAndLabelerIndexFormatConverter"
            }
        },
        {
            "bibmarcconverter" : {
                "_class" : "se.kb.libris.whelks.plugin.MarcBib2JsonLDConverter",
                "_params" : "bib"
            }
        },
        {
            "holdmarcconverter" : {
                "_class" : "se.kb.libris.whelks.plugin.Marc2JsonLDConverter",
                "_params" : "hold"
            }
        },
        {
            "authmarcconverter" : {
                "_class" : "se.kb.libris.whelks.plugin.MarcAuth2JsonLDConverter",
                "_params" : "auth"
            }
        },
        {
            "cleanupconverter" : {
                "_class" : "se.kb.libris.whelks.plugin.JsonLDCleanupIndexFormatConverter",
                "_priority" : 0
            }
        },
        {
            "entityextractor" : {
                "_class" : "se.kb.libris.whelks.plugin.JsonLDEntityExtractorIndexFormatConverter",
                "_priority" : 1
            }
        },
        {
            "jsonldlinkfinder" : {
                "_class" : "se.kb.libris.whelks.plugin.JsonLDLinkFinder"
            }
        }
    ]
}
